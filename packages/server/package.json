{
  "name": "@mustardmind/server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn gen && tsc",
    "gen": "yarn gen:prisma && yarn gen:typegql",
    "gen:prisma": "prisma generate",
    "gen:typegql": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "migrate": "prisma migrate dev --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "2.15.0",
    "@types/graphql-fields": "^1.3.3",
    "apollo-server": "2.19.2",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-type-json": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.15.0",
    "@types/bcrypt": "^3.0.0",
    "@types/node": "12.19.15",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "typegraphql-prisma": "^0.10.1",
    "typescript": "4.1.3"
  }
}
